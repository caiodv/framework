{
  "root" : "/",
  "mode" : "destroy+run",
  "title" : "Utils",
  "services" : [
    {
      "name" : "get_number",
      "url" : "http://echo.jsontest.com/{method}/{value}",
      "type" : "REST",
      "method" : "POST",
      "vars" : {
        "method" : "key",
        "value"  : ":id"
      }
    },
    {
      "name" : "get_json",
      "url" : "http://ip.jsontest.com",
      "type" : "JSON",
      "method" : "POST",
      "vars" : {
        "method" : "key",
        "value"  : "teste"
      }
    },
    {
      "name" : "get_response",
      "url" : "http://ip.jsontest.com",
      "type" : "URL_VARS",
      "method" : "POST",
      "vars" : {
        "method" : "key",
        "value"  : "teste"
      }
    }
  ],
  "sections" : [
    {
      "route" : "/test",
      "model" : "src/app/models/pages/container",
      "controller" : "src/app/controllers/pages/container",
      "view" : "src/app/views/pages/container",
      "template" : "src/templates/pages/container",
      "container" : "#wrap",
      "dependency" : "",
      "dependency_service" : ""
    },
    {
      "route" : "/:method/:id",
      "model" : "src/app/models/pages/index",
      "controller" : "src/app/controllers/pages/index",
      "view" : "src/app/views/pages/index",
      "template" : "src/templates/pages/index",
      "container" : "#pages_container",
      "dependency" : "/test",
      "dependency_service" : "get_number"
    }
  ]
}